    "markdown-it": "^12.3.2",
    "markdown-it-code-copy": "^0.1.1",
    "markdown-it-highlightjs": "^3.6.0",
    "markdown-it-mermaid-plugin": "^0.1.0",
    "markdown-it-texmath": "^0.9.7",
    "mermaid": "^8.13.10",
    "v-click-outside": "^3.1.2",
    "vue": "^2.6.14",
